{
  "name": "telegram-bot-manager",
  "productName": "Telegram bot-manager",
  "version": "1.0.0",
  "description": "Бот-менеджер для телеграм ботов, можно запускать удаленных ботов",
  "main": "main.js",
  "author": "Alexandr_B",
  "license": "MIT",
  "homepage": "https://example.com",
  "scripts": {
    "start": "electron-forge start",
    "make": "electron-forge make",
    "make:win": "electron-forge make --platform win32",
    "make:mac": "electron-forge make --platform darwin",
    "make:linux": "electron-forge make --platform linux",
    "watch:css": "npx @tailwindcss/cli -i ./src/css/input.css -o ./src/css/output.css --watch",
    "dev": "npm run watch:css & npm start",
    "rebuild": "npx electron-rebuild --platform win32"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@iconify/json": "^2.2.418",
    "@iconify/tailwind4": "^1.2.0",
    "electron": "^39.2.7",
    "electron-debug": "^4.1.0",
    "electron-rebuild": "^3.2.9",
    "flyonui": "^2.4.1",
    "jquery": "^3.7.1"
  },
  "dependencies": {
    "@iconify-json/line-md": "^1.2.11",
    "@iconify-json/tabler": "^1.2.24",
    "@iconify/tailwind4": "^1.2.0",
    "better-sqlite3": "^12.5.0",
    "electron-squirrel-startup": "^1.0.1",
    "flyonui": "^2.4.1",
    "socket.io-client": "^4.8.1",
    "sqlite3": "^5.1.7"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "name": "TelegramBotManager",
        "productName": "Telegram Bot Manager",
        "asar": true,
        "icon": "assets/icons/icon"
      },
      "rebuildConfig": {},
      "publishers": [],
      "plugins": [],
      "hooks": {},
      "buildIdentifier": "TelegramBotManager",
      "outDir": "./../dist/",
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "iconUrl": "https://imagetostl.com/Api/V1/Download/Get/8c7a7a0c-f750-424b-8582-2e1f7ec8e2d0/d58f427c-f2ed-4fde-886a-2c6c048f5739/639020646651824712",
            "shortcutName": "Telegram Bot Manager",
            "createDesktopShortcut": true,
            "createStartMenuShortcut": true,
            "setupIcon": "assets/icons/icon.ico",
            "installerTitle": "Telegram Bot Manager Setup",
            "uninstallerTitle": "Telegram Bot Manager Uninstall"
          }
        },
        {
          "name": "@electron-forge/maker-zip"
        }
      ]
    }
  }
}